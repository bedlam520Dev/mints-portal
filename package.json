{
	"$schema": "https://json.schemastore.org/package",
	"name": "mints-portal",
	"version": "1.0.3",
	"private": true,
	"scripts": {
		"dev": "dotenvx run -f .env.development -- next dev",
		"dev:testnet": "dotenvx run -f .env.testnet -- next dev",
		"build": "dotenvx run -f .env.production -- next build",           
		"build:testnet": "dotenvx run -f .env.testnet -- next build",      
		"start": "dotenvx run -f .env.production -- next start",
		"start:testnet": "dotenvx run -f .env.testnet -- next start",
		"clean:cache": "rm -rf .next/dev/cache > _devlogs/clean/clean-cache-$(date +%Y%m%d-%H%M%S).log 2>&1",
		"clean:nuke": "rm -rf .next tsconfig.tsbuildinfo node_modules > _devlogs/clean/clean-nuke-$(date +%Y%m%d-%H%M%S).log 2>&1",
		"tschk": "tsc --noEmit > _devlogs/tschk/tschk-$(date +%Y%m%d-%H%M%S).log 2>&1",
		"lint": "oxlint > _devlogs/oxlint/lint-$(date +%Y%m%d-%H%M%S).log 2>&1",
		"lint:fix": "oxlint --fix > _devlogs/oxlint/lint-fix-$(date +%Y%m%d-%H%M%S).log 2>&1",
		"format:check": "oxfmt --check > _devlogs/oxfmt/fmt-chk-$(date +%Y%m%d-%H%M%S).log 2>&1",
		"format": "oxfmt > _devlogs/oxfmt/fmt-$(date +%Y%m%d-%H%M%S).log 2>&1",
		"validate": "pnpm tschk && pnpm lint && pnpm format > _devlogs/validate/validate-$(date +%Y%m%d-%H%M%S).log 2>&1",
		"analyze": "ANALYZE=true pnpm build > _devlogs/analyze/analyze-$(date +%Y%m%d-%H%M%S).log 2>&1",
		"upgrade:icons": "node tools/upgrade-to-lucide-animated.mjs > _devlogs/animlucide/animate-lucide-$(date +%Y%m%d-%H%M%S).log 2>&1",
		"upgrade:icons:dry": "node tools/upgrade-to-lucide-animated.mjs --dry-run > _devlogs/animlucide/animate-lucide-dry-$(date +%Y%m%d-%H%M%S).log 2>&1"
	},
	"dependencies": {
		"@radix-ui/react-aspect-ratio": "^1.1.8",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-separator": "^1.1.8",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@tailwindcss/postcss": "^4.1.18",
		"@vercel/analytics": "^1.6.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"eruda": "^3.4.3",
		"lucide-react": "^0.563.0",
		"next": "^16.1.4",
		"next-themes": "^0.4.6",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.18",
		"tailwindcss-animate": "^1.0.7",
		"thirdweb": "^5.118.0",
		"vaul": "^1.1.2",
		"viem": "^2.45.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "^1.52.0",
		"@dotenvx/dotenvx-ops": "^0.30.3",
		"@eslint/config-array": "^0.23.0",
		"@eslint/js": "^9.39.2",
		"@hey-api/openapi-ts": "^0.90.10",
		"@next/bundle-analyzer": "^16.1.4",
		"@next/eslint-plugin-next": "^16.1.4",
		"@oxc-project/types": "^0.111.0",
		"@prettier/plugin-oxc": "^0.1.3",
		"@types/node": "^25.0.10",
		"@types/react": "^19.2.9",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@typescript-eslint/parser": "^8.54.0",
		"@walletconnect/ethereum-provider": "^2.23.4",
		"@walletconnect/sign-client": "^2.23.4",
		"@walletconnect/universal-provider": "^2.23.4",
		"eslint": "^9.39.2",
		"eslint-config-next": "^16.1.4",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.5",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"globals": "^17.1.0",
		"motion": "^12.29.2",
		"oxc-minify": "^0.111.0",
		"oxc-parser": "^0.111.0",
		"oxc-resolver": "^11.16.4",
		"oxc-transform": "^0.111.0",
		"oxfmt": "^0.27.0",
		"oxlint": "^1.42.0",
		"postcss": "^8.5.6",
		"prettier": "^3.8.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"qr": "^0.5.4",
		"react-use-measure": "^2.1.7",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0"
	},
	"engines": {
		"node": "24.x"
	},
	"packageManager": "pnpm@10.28.2",
	"pnpm": {
		"overrides": {
			"oxc-parser>@oxc-project/types": "^0.111.0",
			"@oxc-project/types": "^0.111.0",
			"thirdweb>@solana/sysvars": "^5.4.0",
			"@solana/sysvars": "^5.4.0",
			"thirdweb>@hey-api/openapi-ts": "^0.90.4",
			"thirdweb>@paulmillr/qr": "npm:qr@^0.5.4",
			"@paulmillr/qr": "npm:qr@^0.5.4",
			"qr": "^0.5.4",
			"thirdweb>@walletconnect/ethereum-provider": "^2.23.4",
			"@walletconnect/ethereum-provider": "^2.23.4",
			"thirdweb>@walletconnect/sign-client": "^2.23.4",
			"@walletconnect/sign-client": "^2.23.4",
			"thirdweb>@walletconnect/universal-provider": "^2.23.4",
			"@walletconnect/universal-provider": "^2.23.4",
			"@hey-api/client-fetch": "npm:@hey-api/openapi-ts@^0.90.10",
			"@hey-api/openapi-ts": "^0.90.10",
			"wagmi>zod": "^4.3.6",
			"zod": "^4.3.6",
			"valtio>react": "^19.2.3",
			"react": "^19.2.3",
			"@typescript-eslint/eslint-plugin>@typescript-eslint/parser": "^8.54.0",
			"@typescript-eslint/parser": "^8.54.0",
			"eslint>@humanwhocodes/config-array": "npm:@eslint/config-array@^0.23.0",
			"@humanwhocodes/config-array": "npm:@eslint/config-array@^0.23.0",
			"@eslint/config-array": "^0.23.0"
		},
		"onlyBuiltDependencies": [
			"@reown/appkit",
			"bufferutil",
			"esbuild",
			"keccak",
			"sharp",
			"unrs-resolver",
			"utf-8-validate"
		]
	}
}
